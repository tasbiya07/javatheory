package Com.sample;
//Parent class
class Person1 {
String name;
int age;
void showPerson() {
   System.out.println("Name: " + name);
   System.out.println("Age: " + age);
}
}
//Child class
class Employee1 extends Person1 {
int salary;
void showEmployee() {
   showPerson();
   System.out.println("Salary: " + salary);
}
}
class Manager1 extends Employee1 {
String pos;
void showManager() {
   showEmployee();
   System.out.println("Job Role: " + pos);
}
}
//Main class
public class MultiLevelInheritence {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
   Manager1 m = new Manager1();
   m.name = "TOM";
   m.age =24;
   m.salary = 90000;
   m.pos = "ENGINEER";

   m.showManager();
 }
}

OUTPUT:
Name: TOM
Age:24
Salary: 90000
Job Role: ENGINEER
